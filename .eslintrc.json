{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": "off",
        "eslint-disable-next-line":"off",
        "object-curly-newline": "off",
        "arrow-body-style": "off"
    }
}
